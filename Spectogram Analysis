            "Epoch 62/100\n",
            "\u001b[1m5/5\u001b[0m \u001b[32m━━━━━━━━━━━━━━━━━━━━\u001b[0m\u001b[37m\u001b[0m \u001b[1m0s\u001b[0m 22ms/step - accuracy: 0.6260 - loss: 0.9404 - val_accuracy: 0.5000 - val_loss: 1.5652 - learning_rate: 1.2500e-04\n",
            "Epoch 63/100\n",
            "\u001b[1m5/5\u001b[0m \u001b[32m━━━━━━━━━━━━━━━━━━━━\u001b[0m\u001b[37m\u001b[0m \u001b[1m0s\u001b[0m 27ms/step - accuracy: 0.5227 - loss: 1.3120 - val_accuracy: 0.5000 - val_loss: 1.5650 - learning_rate: 1.2500e-04\n",
            "Epoch 64/100\n",
            "\u001b[1m5/5\u001b[0m \u001b[32m━━━━━━━━━━━━━━━━━━━━\u001b[0m\u001b[37m\u001b[0m \u001b[1m0s\u001b[0m 22ms/step - accuracy: 0.6875 - loss: 0.9524 - val_accuracy: 0.5000 - val_loss: 1.5653 - learning_rate: 1.2500e-04\n",
            "Epoch 65/100\n",
            "\u001b[1m5/5\u001b[0m \u001b[32m━━━━━━━━━━━━━━━━━━━━\u001b[0m\u001b[37m\u001b[0m \u001b[1m0s\u001b[0m 23ms/step - accuracy: 0.6523 - loss: 1.0121 - val_accuracy: 0.5000 - val_loss: 1.5678 - learning_rate: 1.2500e-04\n",
            "\u001b[1m4/4\u001b[0m \u001b[32m━━━━━━━━━━━━━━━━━━━━\u001b[0m\u001b[37m\u001b[0m \u001b[1m0s\u001b[0m 11ms/step - accuracy: 0.6336 - loss: 1.2881\n",
            "Final loss: 1.3345859050750732\n",
            "Final accuracy: 0.6100000143051147\n"
          ]
        }
      ],
      "source": [
        "import os\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "import pyarrow.parquet as pq\n",
        "import tensorflow as tf\n",
        "from tensorflow.keras.models import Model\n",
        "from tensorflow.keras.layers import Input, Dense, Dropout, BatchNormalization, LeakyReLU, concatenate\n",
        "from tensorflow.keras.optimizers import Adam\n",
        "from tensorflow.keras.callbacks import EarlyStopping, ReduceLROnPlateau\n",
        "from tensorflow.keras.regularizers import l2\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "import math\n",
        "\n",
        "\n",
        "frequency_bands = {\n",
        "    'delta': (0.5, 4),\n",
        "    'theta': (4, 8),\n",
        "    'alpha': (8, 13),\n",
        "    'beta': (13, 30),\n",
        "    'gamma': (30, 100)\n",
        "}\n",
        "\n",
        "# Extract Features\n",
        "def extract_band_power(spectrogram_values, frequencies, band):\n",
        "    band_start, band_end = frequency_bands[band]\n",
        "    band_indices = np.where((frequencies >= band_start) & (frequencies < band_end))[0]\n",
        "    if len(band_indices) == 0:\n",
        "        return np.zeros(spectrogram_values.shape[0])\n",
        "    band_power = np.mean(spectrogram_values[:, band_indices], axis=1)\n",
        "    return band_power\n",
        "\n",
        "def find_peak_frequencies(frequencies, log_spectrogram_values):\n",
        "    peak_indices = np.argmax(log_spectrogram_values, axis=1)\n",
        "    peak_frequencies = frequencies[peak_indices]\n",
        "    avg_peak_frequency = np.mean(peak_frequencies)\n",
        "    return avg_peak_frequency\n",
        "\n",
        "def process_spectrogram_file(file_path):\n",
        "    spectrogram_data = pd.read_parquet(file_path)\n",
        "    frequencies = np.array([float(col.split('_')[-1]) for col in spectrogram_data.columns[1:]])\n",
        "    spectrogram_values = spectrogram_data.iloc[:, 1:].apply(pd.to_numeric).values\n",
        "    log_spectrogram_values = 10 * np.log10(spectrogram_values + 1e-10)\n",
        "\n",
        "    # Average band powers\n",
        "    band_powers = []\n",
        "    for band in frequency_bands:\n",
        "        power = extract_band_power(log_spectrogram_values, frequencies, band)\n",
        "        band_powers.append(np.nan_to_num(np.mean(power)))\n",
        "\n",
        "    # Other features\n",
        "    avg_peak_frequency = find_peak_frequencies(frequencies, log_spectrogram_values)\n",
        "    avg_amplitude_value = np.nan_to_num(np.mean(spectrogram_values))\n",
        "\n",
        "    return [band_powers, avg_peak_frequency, avg_amplitude_value]\n",
        "\n",
        "# Load dataset\n",
        "def find_eeg_id_and_seizure_vote(folder_path, train_csv_path):\n",
        "    train_df = pd.read_csv(train_csv_path)\n",
        "    consensus_mapping = {\n",
        "        'Other': [0,0,0,0,0,1],\n",
        "        'Seizure': [0,0,0,0,1,0],\n",
        "        'GRDA': [0,0,0,1,0,0],\n",
        "        'GPD': [0,0,1,0,0,0],\n",
        "        'LRDA': [0,1,0,0,0,0],\n",
        "        'LPD': [1,0,0,0,0,0],\n",
        "    }\n",
        "\n",
        "    features_list_x1, features_list_x2, features_list_x3, consensus_list = [], [], [], []\n",
        "\n",
        "    for filename in os.listdir(folder_path):\n",
        "        if filename.endswith(\".parquet\"):\n",
        "            parquet_path = os.path.join(folder_path, filename)\n",
        "            spectrogram_id = int(filename.split('.')[0])\n",
        "            row = train_df[train_df['spectrogram_id'] == spectrogram_id]\n",
        "            if row.empty:\n",
        "                continue\n",
        "\n",
        "            expert_consensus = row.iloc[0]['expert_consensus']\n",
        "            y = consensus_mapping[expert_consensus]\n",
        "            file_features = process_spectrogram_file(parquet_path)\n",
        "\n",
        "            avg_band_powers, avg_peak_frequency, avg_amplitude_value = file_features\n",
        "            features_list_x1.append(avg_band_powers)\n",
        "            features_list_x2.append([avg_peak_frequency])\n",
        "            features_list_x3.append([avg_amplitude_value])\n",
        "            consensus_list.append(y)\n",
        "\n",
        "    # Convert to numpy arrays\n",
        "    X1 = np.array(features_list_x1)\n",
        "    X2 = np.array(features_list_x2)\n",
        "    X3 = np.array(features_list_x3)\n",
        "    Y = np.array(consensus_list)\n",
        "\n",
        "    # Normalize\n",
        "    X1 = StandardScaler().fit_transform(X1)\n",
        "    X2 = StandardScaler().fit_transform(X2)\n",
        "    X3 = StandardScaler().fit_transform(X3)\n",
        "\n",
        "    return X1, X2, X3, Y\n",
        "\n",
        "\n",
        "folder_path = '/content/drive/MyDrive/test'\n",
        "train_csv_path = '/content/drive/MyDrive/train.csv'\n",
        "\n",
        "X1, X2, X3, Y = find_eeg_id_and_seizure_vote(folder_path, train_csv_path)\n",
        "print(\"X1 shape:\", X1.shape)\n",
        "print(\"X2 shape:\", X2.shape)\n",
        "print(\"X3 shape:\", X3.shape)\n",
        "print(\"Y shape:\", Y.shape)\n",
        "\n",
        "input_1 = Input(shape=(X1.shape[1],))\n",
        "input_2 = Input(shape=(X2.shape[1],))\n",
        "input_3 = Input(shape=(X3.shape[1],))\n",
        "\n",
        "# Branch X1\n",
        "x1_branch = Dense(128)(input_1)\n",
        "x1_branch = LeakyReLU(0.1)(x1_branch)\n",
        "x1_branch = BatchNormalization()(x1_branch)\n",
        "x1_branch = Dropout(0.4)(x1_branch)\n",
        "x1_branch = Dense(64)(x1_branch)\n",
        "x1_branch = LeakyReLU(0.1)(x1_branch)\n",
        "\n",
        "# Branch X2\n",
        "x2_branch = Dense(64)(input_2)\n",
        "x2_branch = LeakyReLU(0.1)(x2_branch)\n",
        "x2_branch = BatchNormalization()(x2_branch)\n",
        "x2_branch = Dropout(0.3)(x2_branch)\n",
        "\n",
        "# Branch X3\n",
        "x3_branch = Dense(64)(input_3)\n",
        "x3_branch = LeakyReLU(0.1)(x3_branch)\n",
        "x3_branch = BatchNormalization()(x3_branch)\n",
        "x3_branch = Dropout(0.3)(x3_branch)\n",
        "\n",
        "# Merge\n",
        "merged = concatenate([x1_branch, x2_branch, x3_branch])\n",
        "x = Dense(64)(merged)\n",
        "x = LeakyReLU(0.1)(x)\n",
        "x = Dropout(0.3)(x)\n",
        "x = Dense(32)(x)\n",
        "x = LeakyReLU(0.1)(x)\n",
        "\n",
        "output = Dense(6, activation='softmax')(x)\n",
        "\n",
        "model = Model(inputs=[input_1, input_2, input_3], outputs=output)\n",
        "model.compile(optimizer=Adam(0.0005),\n",
        "              loss='categorical_crossentropy',\n",
        "              metrics=['accuracy'])\n",
        "\n",
        "early_stopping = EarlyStopping(monitor='val_loss', patience=15, restore_best_weights=True)\n",
        "reduce_lr = ReduceLROnPlateau(monitor='val_loss', factor=0.5, patience=5, min_lr=1e-6)\n",
        "\n",
        "history = model.fit([X1, X2, X3], Y,\n",
        "                    epochs=100,\n",
        "                    batch_size=16,\n",
        "                    validation_split=0.2,\n",
        "                    callbacks=[early_stopping, reduce_lr],\n",
        "                    verbose=1)\n",
        "\n",
        "# Evaluate\n",
        "loss, accuracy = model.evaluate([X1, X2, X3], Y)\n",
        "print(\"Final loss:\", loss)\n",
        "print(\"Final accuracy:\", accuracy)\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "model.save(\"/content/spectogram_model.h5\")"
